<?jelly escape-by-default='true'?>

<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
    <f:invisibleEntry>
        <f:textbox  style="display:none" field="id"/>
    </f:invisibleEntry>
    <f:entry title="${%Property file path}" field="propertyPath">
        <f:textbox />
    </f:entry>
    <f:entry title="${%Username key}" field="usernameKey">
        <f:textbox />
    </f:entry>
    <f:entry title="${%Password key}" field="passwordKey">
        <f:textbox />
    </f:entry>
    <f:entry title="${%Description}" field="description">
        <f:textbox/>
    </f:entry>
</j:jelly>
